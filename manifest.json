{
    "manifest_version": 3,
    "name": "LinkedIn Jobs to CSV",
    "version": "1.0",
    "description": "Export LinkedIn Saved Jobs to CSV",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_title": "LinkedIn Jobs to CSV",
        "default_popup": "popup.html",
        "default_icon": "images/icon48.png"
    },
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "background": {
        "service_worker": "background.js"
    }
}